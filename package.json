{
  "name": "flight-scraper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "npx tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/* && npm run clean:logs",
    "clean:logs": "rm -rf logs/*",
    "clean:docker": "bash minikube/cleanup.sh",
    "lint": "npx eslint --fix",
    "prebuild": "npm run clean && npm run lint && npm install",
    "build": "npm run tsc && cd frontend && npm run build",
    "postbuild": "cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "start": "NODE_ENV=production MONGODB_URI='mongodb://0.0.0.0:27017/flight' node ./dist/index.js",
    "dev:start": "ts-node src/index.ts",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "dev:watch": "nodemon --watch src --exec ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b-bly/flight-scraper.git"
  },
  "author": "Brendt Bly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/b-bly/flight-scraper/issues"
  },
  "homepage": "https://github.com/b-bly/flight-scraper#readme",
  "dependencies": {
    "14": "^3.1.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "install-chrome-dependencies": "^1.1.2",
    "mongoose": "^6.3.4",

    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/d3-fetch": "^3.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/puppeteer": "^5.4.6",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  }
}
